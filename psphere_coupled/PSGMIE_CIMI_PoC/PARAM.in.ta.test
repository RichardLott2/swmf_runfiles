5.0			        DtSaveLogfile
dt rho mx my mz p bx by bz pmin pmax dst cpcpn
3.0 3.5 5.0		  StringRFlux

#SAVEPLOT
2               nPlotFiles
idl y=0 VAR     StringPlot
-1              DnSavePlot
1.             DtSavePlot
-1              DxSavePlot
rho ux uy uz bx by bz pe p hprho hpmx hpmy hpmz hpp hppsrho hppsmx hppsmz hppsp jx jy jz
g rbody
idl z=0 VAR     StringPlot
-1              DnSavePlot
1.             DtSavePlot
-1              DxSavePlot
rho ux uy uz bx by bz pe p hprho hpmx hpmy hpmz hpp hppsrho hppsmx hppsmz hppsp jx jy jz
g rbody

#SCHEME
2			nORDER
Rusanov			TypeFlux
mc3                     TypeLimiter
1.2			BetaLimiter

#BORIS
T
0.01

#REGION
borisregion             NameArea
sphere tapered          NameShape
-4.0			xPosition
0.0			yPosition
0.0			zPosition
14.0			Radius
2.0			Taper

#BORISREGION
borisregion			NameBorisRegion

#TIMESTEPPING
1			nSTAGE
0.85			cfl

#OUTERBOUNDARY
outflow			TypeBC_I(east_)
vary			TypeBC_I(west_)
